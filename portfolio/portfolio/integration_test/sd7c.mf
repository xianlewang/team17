;intialize 
0s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_BL_CAN_ID DoorClosed BACK LEFT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_BR_CAN_ID DoorClosed BACK RIGHT = true

0s I MESSAGE_PERIOD N HALLCALL_1FU_CAN_ID HallCall 1 FRONT UP = true
0s I MESSAGE_PERIOD N HALLCALL_7FU_CAN_ID HallCall 7 FRONT UP = false

0s I MESSAGE_PERIOD N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT = true
0s I MESSAGE_PERIOD N AT_FLOOR_[2][BACK]_CAN_ID AtFloor 2 BACK = false
0s I MESSAGE_PERIOD N AT_FLOOR_[3][FRONT]_CAN_ID AtFloor 3 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[4][FRONT]_CAN_ID AtFloor 4 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[5][FRONT]_CAN_ID AtFloor 5 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[6][FRONT]_CAN_ID AtFloor 6 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[7][FRONT]_CAN_ID AtFloor 7 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[8][FRONT]_CAN_ID AtFloor 8 FRONT = false

0s I MESSAGE_PERIOD N LEVEL_SENSOR_[UP]_CAN_ID Leveling UP = true
0s I MESSAGE_PERIOD N LEVEL_SENSOR_[DOWN]_CAN_ID Leveling DOWN = true
0s I MESSAGE_PERIOD N HOISTWAY_LIMIT_CAN_ID HoistwayLimitSensor UP = false
0.4s A F DoorMotor FRONT LEFT : command == OPEN
0.5s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = false
0.5s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = false
0.5s I MESSAGE_PERIOD N DOOR_CLOSED_BL_CAN_ID DoorClosed BACK LEFT = false
0.5s I MESSAGE_PERIOD N DOOR_CLOSED_BR_CAN_ID DoorClosed BACK RIGHT = false

;****verify precondition
1s A N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT : getValue == false
1s A F DoorMotor FRONT LEFT : command == OPEN
1s A N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT : getValue == true



;#arc '7C/1'
1s I MESSAGE_PERIOD N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT = true
1.5s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][LEFT]_CAN_ID DoorOpened FRONT LEFT = true
1.5s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][RIGHT]_CAN_ID DoorOpened FRONT RIGHT = true
1.5s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[BACK][LEFT]_CAN_ID DoorOpened BACK LEFT = true
1.5s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[BACK][RIGHT]_CAN_ID DoorOpened BACK RIGHT = true
2.0s I MESSAGE_PERIOD N HALLCALL_1FU_CAN_ID HallCall 1 FRONT UP = false

;#arc '7C/2'
7.0s A F DoorMotor FRONT LEFT : command == CLOSE
7.0s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][LEFT]_CAN_ID DoorOpened FRONT LEFT = false
7.0s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][RIGHT]_CAN_ID DoorOpened FRONT RIGHT = false
7.0s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[BACK][LEFT]_CAN_ID DoorOpened BACK LEFT = false
7.0s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[BACK][RIGHT]_CAN_ID DoorOpened BACK RIGHT = false

;#arc '7C/3a'
10s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = true
10s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = true
10s I MESSAGE_PERIOD N DOOR_CLOSED_BL_CAN_ID DoorClosed BACK LEFT = true
10s I MESSAGE_PERIOD N DOOR_CLOSED_BR_CAN_ID DoorClosed BACK RIGHT = true

;#arc '7C/3b'
11.0s A F DoorMotor FRONT LEFT : command == STOP
11.0s A F DoorMotor FRONT RIGHT : command == STOP
11.0s A F DoorMotor BACK LEFT : command == STOP
11.0s A F DoorMotor BACK RIGHT : command == STOP

;#arc '7C/4a'
12.0s I MESSAGE_PERIOD N HALLCALL_7FU_CAN_ID HallCall 7 FRONT UP = true
15.5s A N DESIRED_FLOOR_CAN_ID DesiredFloor : getFloor == 7
15.5s A N DESIRED_FLOOR_CAN_ID DesiredFloor : getDirection == UP

;#arc '7C/4b'
16s A F Drive : speed == SLOW
16s I MESSAGE_PERIOD N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT = false

;#arc '7C/5a'
17.0s I MESSAGE_PERIOD N HOISTWAY_LIMIT_CAN_ID HoistwayLimitSensor UP = true

;#arc '7C/5b'
17.3s I MESSAGE_PERIOD N AT_FLOOR_[7][FRONT]_CAN_ID AtFloor 7 FRONT = true
17.5s A F Drive : speed == STOP
17.5s A S DriveControl : STATE == STOP
