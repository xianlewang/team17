;network message period
#DEFINE MESSAGE_PERIOD 100ms

#DEFINE AT_FLOOR_[1][FRONT]_CAN_ID 0xC4C2800
#DEFINE AT_FLOOR_[1][BACK]_CAN_ID 0xC4C2801
#DEFINE AT_FLOOR_[2][BACK]_CAN_ID 0xC4C2803
#DEFINE AT_FLOOR_[3][FRONT]_CAN_ID 0xC4C2804
#DEFINE AT_FLOOR_[4][FRONT]_CAN_ID 0xC4C2806
#DEFINE AT_FLOOR_[5][FRONT]_CAN_ID 0xC4C2808
#DEFINE AT_FLOOR_[6][FRONT]_CAN_ID 0xC4C280A
#DEFINE AT_FLOOR_[7][FRONT]_CAN_ID 0xC4C280C
#DEFINE AT_FLOOR_[7][BACK]_CAN_ID 0xC4C280D
#DEFINE AT_FLOOR_[8][FRONT]_CAN_ID 0xC4C280E

#DEFINE DOOR_MOTOR_COMMAND_[FRONT][LEFT]_CAN_ID 0xF0EB800
#DEFINE DOOR_MOTOR_COMMAND_[FRONT][RIGHT]_CAN_ID 0xF0EB801
#DEFINE DOOR_MOTOR_COMMAND_[BACK][LEFT]_CAN_ID 0xF0EB802
#DEFINE DOOR_MOTOR_COMMAND_[BACK][RIGHT]_CAN_ID 0xF0EB803

#DEFINE LEVEL_SENSOR_[UP]_CAN_ID 0xAE81400
#DEFINE LEVEL_SENSOR_[DOWN]_CAN_ID 0xAE81401

;mDesiredFloor
;ID = 0x0F0CB600
#DEFINE DESIRED_FLOOR_CAN_ID 0xF0CB600 

;mDoorClosed
#DEFINE DOOR_CLOSED_FL_CAN_ID 0xD145000
#DEFINE DOOR_CLOSED_FR_CAN_ID 0xD145001
#DEFINE DOOR_CLOSED_BL_CAN_ID 0xD145002
#DEFINE DOOR_CLOSED_BR_CAN_ID 0xD145003

;mDriveSpeed = 0x0F0AB500
#DEFINE DRIVE_COMMAND_CAN_ID 0x0F0AB500

;mDoorOpened = 0x0EA4A000 + 0 + 0 = 0x0EA4A001
#DEFINE DOOR_OPEN_SENSOR_[FRONT][LEFT]_CAN_ID  0x0EA4A000
#DEFINE DOOR_OPEN_SENSOR_[FRONT][RIGHT]_CAN_ID 0xEA4A001
#DEFINE DOOR_OPEN_SENSOR_[BACK][LEFT]_CAN_ID 0xEA4A002
#DEFINE DOOR_OPEN_SENSOR_[BACK][RIGHT]_CAN_ID 0xEA4A003
;mCarWeight = 0x0DDC7800
#DEFINE CAR_WEIGHT_CAN_ID  0x0DDC7800

;intialize  
0s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_BL_CAN_ID DoorClosed BACK LEFT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_BR_CAN_ID DoorClosed BACK RIGHT = true

0s I MESSAGE_PERIOD N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[2][BACK]_CAN_ID AtFloor 2 BACK = false
0s I MESSAGE_PERIOD N AT_FLOOR_[3][FRONT]_CAN_ID AtFloor 3 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[4][FRONT]_CAN_ID AtFloor 4 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[5][FRONT]_CAN_ID AtFloor 5 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[6][FRONT]_CAN_ID AtFloor 6 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[7][FRONT]_CAN_ID AtFloor 7 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[8][FRONT]_CAN_ID AtFloor 8 FRONT = false

0s I MESSAGE_PERIOD F HallCall 4 FRONT UP  = true
0s I MESSAGE_PERIOD N DESIRED_FLOOR_CAN_ID DesiredFloor = 4 FRONT UP
0s I MESSAGE_PERIOD N DRIVE_COMMAND_CAN_ID DriveCommand = SLOW UP
0.5s I MESSAGE_PERIOD F HallCall 4 FRONT UP  = false


;****verify precondition
1s A N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT : getValue == true
1s A N DRIVE_COMMAND_CAN_ID DriveCommand : getDirection == UP
1s A F HallLight 4 FRONT UP : lighted == true ;

;#arc '3A/1a'
2s I MESSAGE_PERIOD N AT_FLOOR_[4][FRONT]_CAN_ID AtFloor 4 FRONT = true
2s I MESSAGE_PERIOD N LEVEL_SENSOR_[UP]_CAN_ID Leveling UP = true
2s I MESSAGE_PERIOD N LEVEL_SENSOR_[DOWN]_CAN_ID Leveling DOWN = true
2s I MESSAGE_PERIOD N DRIVE_COMMAND_CAN_ID DriveCommand = STOP STOP
;#arc '3A/1b'
4s A F HallLight 4 FRONT UP : lighted == false ;


;#arc '3A/2a'
3s A N AT_FLOOR_[4][FRONT]_CAN_ID AtFloor 4 FRONT : getValue == true
;#arc '3A/2b'
3s A F DoorMotor FRONT LEFT : command == OPEN
3.2s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = false
3.2s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = false
3.2s I MESSAGE_PERIOD N DESIRED_FLOOR_CAN_ID DesiredFloor = 5 FRONT UP

;#arc '3A/3a'
5s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][LEFT]_CAN_ID DoorOpened FRONT LEFT = true
5s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][RIGHT]_CAN_ID DoorOpened FRONT RIGHT = true
;#arc '3A/3b'
5.10s A F DoorMotor FRONT LEFT : command == STOP

;#arc '3A/4'
6.1s A F DoorMotor FRONT LEFT : command == NUDGE
6.25s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][LEFT]_CAN_ID DoorOpened FRONT LEFT = false
6.25s I MESSAGE_PERIOD N DOOR_OPEN_SENSOR_[FRONT][RIGHT]_CAN_ID DoorOpened FRONT RIGHT = false
;#arc '3A/5a'
6.4s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = true
6.4s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = true

;#arc '3A/5b'
6.5s A F DoorMotor FRONT LEFT : command == STOP


;****verify postcondition
7s A N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT : getValue == true
7s A N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT : getValue == true
7s A N DOOR_CLOSED_BL_CAN_ID DoorClosed BACK LEFT : getValue == true
7s A N DOOR_CLOSED_BR_CAN_ID DoorClosed BACK RIGHT : getValue == true
7s A N AT_FLOOR_[4][FRONT]_CAN_ID AtFloor 4 FRONT : getValue == true
7s A F HallLight 4 FRONT UP : lighted == false ;