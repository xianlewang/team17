#DEFINE DOOR_MOTOR_COMMAND_[FRONT][RIGHT]_CAN_ID 0x0BEA6501
#DEFINE DOOR_MOTOR_COMMAND_[BACK][LEFT]_CAN_ID 0x0BEA6502
#DEFINE DOOR_MOTOR_COMMAND_[BACK][RIGHT]_CAN_ID 0x0BEA6503

;mDesiredFloor
;ID = 0x0C4E7000
#DEFINE DESIRED_FLOOR_CAN_ID 0x0C4E7000 

;mDoorClosed
#DEFINE DOOR_CLOSED_FL_CAN_ID 0x0CB27900
#DEFINE DOOR_CLOSED_FR_CAN_ID 0x0CB27901
#DEFINE DOOR_CLOSED_BL_CAN_ID 0x0CB27902
#DEFINE DOOR_CLOSED_BR_CAN_ID 0x0CB27903
;mDriveSpeed = 0x0F0AB500
;#DEFINE DRIVE_COMMAND_CAN_ID 0x0BE86400

;mDoorOpened = 0x0D157C00 + 0 + 0 = 0x0EA4A001
#DEFINE DOOR_OPENED_FL_CAN_ID  0x0D157C00
#DEFINE DOOR_OPENED_FR_CAN_ID 0x0D157C01

;mCarWeight = 0x0D187E00
#DEFINE CAR_WEIGHT_CAN_ID  0x0D187E00

#DEFINE LEVEL_SENSOR_[UP]_CAN_ID 0x09F40A00
#DEFINE LEVEL_SENSOR_[DOWN]_CAN_ID 0x09F40A01

#DEFINE HALLCALL_1FU_CAN_ID 0x0CB17800
#DEFINE HALLCALL_3FU_CAN_ID 0x0CB17808

;intialize 
0s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_BL_CAN_ID DoorClosed BACK LEFT = true
0s I MESSAGE_PERIOD N DOOR_CLOSED_BR_CAN_ID DoorClosed BACK RIGHT = true

0s I MESSAGE_PERIOD N HALLCALL_1FU_CAN_ID HallCall 1 FRONT UP = true
0s I MESSAGE_PERIOD N HALLCALL_3FU_CAN_ID HallCall 3 FRONT UP = false
0s I MESSAGE_PERIOD N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT = true
0s I MESSAGE_PERIOD N AT_FLOOR_[2][BACK]_CAN_ID AtFloor 2 BACK = false
0s I MESSAGE_PERIOD N AT_FLOOR_[3][FRONT]_CAN_ID AtFloor 3 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[4][FRONT]_CAN_ID AtFloor 4 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[5][FRONT]_CAN_ID AtFloor 5 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[6][FRONT]_CAN_ID AtFloor 6 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[7][FRONT]_CAN_ID AtFloor 7 FRONT = false
0s I MESSAGE_PERIOD N AT_FLOOR_[8][FRONT]_CAN_ID AtFloor 8 FRONT = false

0s I MESSAGE_PERIOD N LEVEL_SENSOR_[UP]_CAN_ID Leveling UP = true
0s I MESSAGE_PERIOD N LEVEL_SENSOR_[DOWN]_CAN_ID Leveling DOWN = true
0.2s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = false
0.2s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = false

;****verify precondition
1s A N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT : getValue == false
1s A F DoorMotor FRONT LEFT : command == OPEN
1s A N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT : getValue == true

;#arc '9A/1a'
1s I MESSAGE_PERIOD N AT_FLOOR_[1][FRONT]_CAN_ID AtFloor 1 FRONT = true
1s I MESSAGE_PERIOD N LEVEL_SENSOR_[UP]_CAN_ID Leveling UP = true
1s I MESSAGE_PERIOD N LEVEL_SENSOR_[DOWN]_CAN_ID Leveling DOWN = true

;#arc '9A/1b'
1.2s A F DoorMotor FRONT LEFT : command == OPEN
1.2s A S DoorControl[FRONT][LEFT] : STATE == BEFORE_OPEN

;#arc '9A/2'
1.2s I MESSAGE_PERIOD N HALLCALL_1FU_CAN_ID HallCall 1 FRONT UP = false
1.2s I MESSAGE_PERIOD N HALLCALL_3FU_CAN_ID HallCall 3 FRONT UP = true
1.5s A N DESIRED_FLOOR_CAN_ID DesiredFloor : getFloor == 3
1.5s A N DESIRED_FLOOR_CAN_ID DesiredFloor : getDirection == UP


;#arc '9A/3'
1.5s I MESSAGE_PERIOD N DOOR_OPENED_FL_CAN_ID DoorOpened FRONT LEFT = true
1.5s I MESSAGE_PERIOD N DOOR_OPENED_FR_CAN_ID DoorOpened FRONT RIGHT = true

;#arc '9A/4'
2.0s A F DoorMotor FRONT LEFT : command == STOP

;#arc '9A/5'
17.0s A F DoorMotor FRONT LEFT : command == CLOSE
17.0s I MESSAGE_PERIOD N DOOR_OPENED_FL_CAN_ID DoorOpened FRONT LEFT = false

;#arc '9A/6a'
18s I MESSAGE_PERIOD N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT = true
18s I MESSAGE_PERIOD N DOOR_CLOSED_FR_CAN_ID DoorClosed FRONT RIGHT = true

;#arc '9A/6b'
18.2s A F DoorMotor FRONT LEFT : command == STOP

;#arc '9A/7a'
18.5s A N DESIRED_FLOOR_CAN_ID DesiredFloor : getFloor == 3
18.5s A N DESIRED_FLOOR_CAN_ID DesiredFloor : getDirection == UP

;#arc '9A/7b'
19s A F Drive : speed == SLOW

;****verify postcondition
20s A N DOOR_CLOSED_FL_CAN_ID DoorClosed FRONT LEFT : getValue == true
20.5s A F DoorMotor FRONT LEFT : command == STOP
20.5s A F Drive : direction == UP
