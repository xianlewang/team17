;can ID's used:
;AtFloor[1][Front] = 0x0C4D6F00 + 0 + 0 = 0x0C4D6F00
#DEFINE ATFLOOR_1F_FRONT_CAN_ID 0x0C4D6F00
;AtFloor[1][Back] = 0x0C4D6F00 + 0 + 1 = 0x0C4D6F01
#DEFINE ATFLOOR_1F_BACK_CAN_ID 0x0C4D6F01
;AtFloor[2][Front] = 0x0C4D6F00 + 1*2 + 0 = 0x0C4D6F02
#DEFINE ATFLOOR_2F_FRONT_CAN_ID 0x0C4D6F02
;AtFloor[2][Back] = 0x0C4D6F00 + 1*2 + 1 = 0x0C4D6F03
#DEFINE ATFLOOR_2F_BACK_CAN_ID 0x0C4D6F03
;AtFloor[3][Front] = 0x0C4D6F00 + 2*2 + 0 = 0x0C4D6F04
#DEFINE ATFLOOR_3F_FRONT_CAN_ID 0x0C4D6F04
;AtFloor[3][Back] = 0x0C4D6F00 + 2*2 + 1 = 0x0C4D6F05
#DEFINE ATFLOOR_3F_BACK_CAN_ID 0x0C4D6F05
;AtFloor[4][Front] = 0x0C4D6F00 + 3*2 + 0 = 0x0C4D6F06
#DEFINE ATFLOOR_4F_FRONT_CAN_ID 0x0C4D6F06
;AtFloor[4][Back] = 0x0C4D6F00 + 3*2 + 1 = 0x0C4D6F07
#DEFINE ATFLOOR_4F_BACK_CAN_ID 0x0C4D6F07
;AtFloor[5][Front] = 0x0C4D6F00 + 4*2 + 0 = 0x0C4D6F08
#DEFINE ATFLOOR_5F_FRONT_CAN_ID 0x0C4D6F08
;AtFloor[5][Back] = 0x0C4D6F00 + 4*2 + 1 = 0x0C4D6F09
#DEFINE ATFLOOR_5F_BACK_CAN_ID 0x0C4D6F09
;AtFloor[6][Front] = 0x0C4D6F00 + 5*2 + 0 = 0x0C4D6F0A
#DEFINE ATFLOOR_6F_FRONT_CAN_ID 0x0C4D6F0A
;AtFloor[6][Back] = 0x0C4D6F00 + 5*2 + 1 = 0x0C4D6F0B
#DEFINE ATFLOOR_6F_BACK_CAN_ID 0x0C4D6F0B
;AtFloor[7][Front] = 0x0C4D6F00 + 6*2 + 0 = 0x0C4D6F0C
#DEFINE ATFLOOR_7F_FRONT_CAN_ID 0x0C4D6F0C
;AtFloor[7][Back] = 0x0C4D6F00 + 6*2 + 1 = 0x0C4D6F0D
#DEFINE ATFLOOR_7F_BACK_CAN_ID 0x0C4D6F0D
;AtFloor[8][Front] = 0x0C4D6F00 + 7*2 + 0 = 0x0C4D6F0E
#DEFINE ATFLOOR_8F_FRONT_CAN_ID 0x0C4D6F0E
;AtFloor[8][Back] = 0x0C4D6F00 + 7*2 + 1 = 0x0C4D6F0F
#DEFINE ATFLOOR_8F_BACK_CAN_ID 0x0C4D6F0F

;mDesiredFloor = 0x0C4E7000
#DEFINE DESIRED_FLOOR_CAN_ID  0x0C4E7000

;mDoorClosed[Front][Left] = 0x0CB27900 + 0 + 0 = 0x0CB27900
#DEFINE DOOR_CLOSED_FRONT_LEFT_CAN_ID  0x0CB27900
;mDoorClosed[Front][Right] = 0x0CB27900 + 0 + 1 = 0x0CB27901
#DEFINE DOOR_CLOSED_FRONT_RIGHT_CAN_ID  0x0CB27901
;mDoorClosed[Back][Left] = 0x0CB27900 + 1*2 + 0 = 0x0CB27902
#DEFINE DOOR_CLOSED_BACK_LEFT_CAN_ID  0x0CB27902
;mDoorClosed[Back][Right] = 0x0CB27900 + 1*2 + 1 = 0x0CB27903
#DEFINE DOOR_CLOSED_BACK_RIGHT_CAN_ID  0x0CB27903

;message period
#DEFINE MESSAGE_PERIOD 0.01s

